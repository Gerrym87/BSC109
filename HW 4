        "def main():\n",
        "  quantity = int(input('enter the amount of packages purchased:'))\n",
        "  reg = 99.00\n",
        "  if quantity <= 9 and quantity >= 1:\n",
        "    print('no discount.')\n",
        "    discount=1.0\n",
        "  elif quantity <= 19 and quantity >= 10:\n",
        "    print('your discount is 10%.')\n",
        "    discount=.1\n",
        "  elif quantity <= 49 and quantity >= 20:\n",
        "    print('your discount is 20%.')\n",
